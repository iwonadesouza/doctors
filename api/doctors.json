openapi: 3.0.0
info:
  title: Doctor Directory API
  version: 1.0.0
  description: API to retrieve doctor information based on specialization and location.

paths:
  /doctors:
    get:
      summary: Get a list of doctors
      description: Retrieve a list of doctors with optional filters for specialization and location.
      parameters:
        - in: query
          name: specialization
          schema:
            type: string
          description: Filter by medical specialization
        - in: query
          name: location
          schema:
            type: string
          description: Filter by city/location
      responses:
        '200':
          description: A list of doctors
          content:
            application/json:
              schema:
                type: object
                properties:
                  doctors:
                    type: array
                    items:
                      type: object
                      properties:
                        id:
                          type: string
                        name:
                          type: string
                        specialization:
                          type: string
                        location:
                          type: string
